import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int sum=0,num=n;
         while(n>0){
                int rem=n%10;
                sum=sum+rem;
                n=n/10;
         }
        if(num%sum==0){
            System.out.println("Harshad Number");
            
        }
        else{
           System.out.println("Not Harshad Number"); 
        }
        
    }
}
