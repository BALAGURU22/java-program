import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
         int n,sum=0;
        do {
             n=sc.nextInt();
           
           
         
            sum=sum+n;
      }
        while(n!=-1);
        
            System.out.print(sum+1);
        
    }
}
